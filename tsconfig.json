{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "noImplicitAny": true,
    "jsx": "react-native",
    "lib": ["ESNext"],
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "node",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts"]
}
