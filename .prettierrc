{
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": true,
  "trailingComma": "all",
  "printWidth": 100,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "pluginSearchDirs": ["."],
  "importOrder": ["^react", "^react-native", "^expo", "<THIRD_PARTY_MODULES>", "", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderCaseSensitive": false
}
